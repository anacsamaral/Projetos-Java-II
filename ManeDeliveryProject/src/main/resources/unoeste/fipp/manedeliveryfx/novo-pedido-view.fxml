<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="623.0" prefWidth="1190.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.manedeliveryfx.NovoPedidoController">
   <children>
      <VBox maxHeight="230.0" minHeight="230.0" prefHeight="230.0" prefWidth="100.0" style="-fx-background-color: gray; -fx-background-radius: 8;">
         <children>
            <Label text="Cliente" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="8.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="tfCliente" />
            <GridPane hgap="12.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="CEP" textFill="WHITE" />
                  <TextField fx:id="tfCep" onKeyTyped="#onBuscarCep" GridPane.rowIndex="1" />
                  <Label text="Endereço" textFill="WHITE" GridPane.columnIndex="1" />
                  <Label text="Número" textFill="WHITE" GridPane.columnIndex="2" />
                  <TextField fx:id="tfEndereco" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="tfNumero" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <GridPane hgap="12.0" layoutX="18.0" layoutY="72.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label text="Telefone" textFill="WHITE" />
                  <TextField fx:id="tfTelefone" GridPane.rowIndex="1" />
                  <Label text="Tipo de Pagamento:" textFill="WHITE" GridPane.columnIndex="1" />
                  <ComboBox fx:id="cbTipoPagamento" prefHeight="25.0" prefWidth="1171.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="49.0" prefWidth="1166.0">
               <children>
                  <Label text="Total do Pedido R$  " textFill="#b8ee06">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="lbTotal" text="0,00">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="18.0" />
         </VBox.margin>
      </VBox>
      <VBox alignment="CENTER" prefHeight="365.0" prefWidth="791.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" maxHeight="40.0" minHeight="40.0" prefHeight="40.0" prefWidth="1182.0" spacing="16.0">
               <children>
                  <Button fx:id="btProduto" mnemonicParsing="false" onAction="#onSelProduto" prefHeight="32.0" prefWidth="620.0" style="-fx-background-color: white; -fx-border-color: lightgray; -fx-border-radius: 5;" text="Selecione o produto" textFill="#908989">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="CLOSED_HAND" />
                     </cursor>
                  </Button>
                  <Spinner fx:id="spQuant" prefHeight="30.0" prefWidth="58.0" />
                  <Button mnemonicParsing="false" onAction="#onAdicItem" prefWidth="32.0" style="-fx-background-color: royalblue;" text="+" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
                  <Button layoutX="1001.0" layoutY="22.0" mnemonicParsing="false" onAction="#onDelItem" prefWidth="32.0" style="-fx-background-color: orange;" text="-" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="4.0" />
               </VBox.margin>
            </HBox>
            <TableView fx:id="tableView" prefHeight="300.0" prefWidth="1182.0" style="-fx-background-radius: 4; -fx-border-radius: 4; -fx-border-color: black;" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="coQuant" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="Quantidade" />
                <TableColumn fx:id="coProduto" prefWidth="75.0" text="Produto" />
                  <TableColumn fx:id="coValor" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="Valor" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children></VBox>
      <HBox alignment="CENTER" maxHeight="60.0" minHeight="60.0" prefHeight="60.0" prefWidth="200.0" spacing="120.0" style="-fx-background-color: gray; -fx-background-radius: 8;">
         <children>
            <Button mnemonicParsing="false" onAction="#onConfirmar" prefHeight="28.0" prefWidth="100.0" style="-fx-background-color: beige; -fx-background-radius: 5;" text="Confirmar" />
            <Button mnemonicParsing="false" onAction="#onCancelar" prefHeight="28.0" prefWidth="100.0" style="-fx-background-color: beige; -fx-background-radius: 5;" text="Cancelar" />
         </children>
         <VBox.margin>
            <Insets top="8.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
   </padding>
</VBox>
