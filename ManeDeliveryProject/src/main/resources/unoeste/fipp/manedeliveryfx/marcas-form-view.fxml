<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="408.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.manedeliveryfx.MarcasFormController">
    <children>
        <Label text="ID" textFill="#2768eb">
            <font>
                <Font size="11.0" />
            </font>
        </Label>
        <TextField fx:id="tfId" editable="false" />
        <Label layoutX="10.0" layoutY="10.0" text="Nome da Marca" textFill="#2768eb">
            <font>
                <Font size="11.0" />
            </font>
            <VBox.margin>
                <Insets top="16.0" />
            </VBox.margin>
        </Label>
        <TextField fx:id="tfNome" layoutX="10.0" layoutY="108.0" />
        <HBox alignment="CENTER" prefHeight="108.0" prefWidth="570.0" spacing="60.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onConfirmar" prefHeight="33.0" prefWidth="83.0" style="-fx-background-color: white; -fx-border-color: gray; -fx-border-radius: 8;" text="Confirmar" />
                <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onCancelar" prefHeight="33.0" prefWidth="83.0" style="-fx-background-color: white; -fx-border-color: gray; -fx-border-radius: 8;" text="Cancelar" />
            </children>
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
    </padding>
</VBox>
